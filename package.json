  "license": "BSD-3-Clause",
  "scripts": {
    "start": "vite",
    "build": "rm -rf ./dist/ && vite build",
    "build:package": "deno task build && deno task package",
    "package": "rm -rf ./build/ && deno ./bin/package.ts",
    "serve": "vite preview",
    "test": "deno test || echo 'No tests defined yet'",
    "test:coverage": "deno task test --coverage",
    "bumpManifestVer": "deno -A ./bin/bumpManifestVer.ts"
  },
  "dependencies": {
    "@deskpro/app-sdk": "^6.0.3",
    "@deskpro/deskpro-ui": "^8.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^4.1.2",
    "react-resize-observer": "^1.1.1",
    "react-router-dom": "^6.29.0",
    "simplebar": "^6.3.0",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.26",
    "@vitejs/plugin-react": "^1.3.2",
    "rollup-plugin-copy": "3.4.0",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.15",
    "vite": "^6.1.0"
  }
}
